<%@ page language="java" contentType="text/html; charset=US-ASCII"
	pageEncoding="US-ASCII"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Registration</title>
<meta name="keywords" content="" />
<meta name="description" content="" />
<link
	href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700|Archivo+Narrow:400,700"
	rel="stylesheet" type="text/css">
<link href="register.css" rel="stylesheet" type="text/css" media="all" />
<!--[if IE 6]>
<link href="default_ie6.css" rel="stylesheet" type="text/css" />
<![endif]-->
</head>

<body>
	<div id="header-wrapper">
		<div id="header" class="container">
			<div id="logo">
				<h1>
					<a href="/FamilyTree/welcome.jsp">My Family Tree</a>
				</h1>
			</div>
			<div id="menu">
				<ul>
					<li class="active"><a href="/FamilyTree/welcome.jsp"
						accesskey="1" title="">Home</a></li>
				</ul>
			</div>
		</div>
	</div>

	<div id="banner-wrapper">
		<ul style="display: inline">
			<li>
				<div id="banner" class="regForm">

					<div align="left">
						<form method="post" action="register.html" class="register">
							<p>
								<label for="name">Name:</label> <input type="text" name="name"
									id="name" value="" />
							</p>
							<p>
								<label for="email">Email:</label> <input type="text"
									name="email" id="email" value="" />
							</p>
							<p>
								<label for="password">Password:</label> <input type="password"
									name="password" id="password" value="">
							</p>
							<p>
								<label for="confirmPassword">Re-enter:</label> <input
									type="password" name="confirmPassword" id="confirmPassword"
									value="">
							</p>

							<div id="regSubmit">
								<p align="left" id="link">
									<input type="checkbox" name="link" id="link" value="">
									I'm already part of a tree
								</p>
								<a href="#" class="button">Register</a>
							</div>
						</form>
					</div>
				</div>
			</li>
			<li>
				<div id="treePicture">
					<img src="images/familytree.png" width="623" />
					<p>
						Your Family Ancestry just <strong>a click away</strong>.
					</p>
				</div>
			</li>
		</ul>
	</div>

	<div id="copyright" class="container">
		<p>
			Copyright (c) 2013 <a href="http://www.familytree.com">FamilyTree.com</a>.
			All rights reserved.
		</p>
	</div>
</body>
</html>





