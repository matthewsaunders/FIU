package account.models;

public class Comments {

    public int id;
    public int parent;
    public String date;
    public String owner;
    public String text;
    public String fname;
    public String lname;

    public Comments(int id,int parent,String date,String owner, String text,String fname, String lname) {
        this.id = id;
        this.parent = parent;
        this.date = date;
        this.owner = owner;
        this.text = text;
        this.fname = fname;
        this.lname = lname;
    }
    
    public int getID(){
    	return this.id;
    }
    
    public int getParent(){
    	return this.parent;
    }
    
    public String getDate(){
    	return this.date;
    }
    
    public String getOwner(){
    	return this.owner;
    }
    
    public String getText(){
    	return this.text;
    }
    
    public String getFname(){
    	return this.fname;
    }
    
    public String getLname(){
    	return this.lname;
    }
}
